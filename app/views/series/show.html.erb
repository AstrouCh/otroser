<div class="head-serie">
  <H1 id="otroser-title"> <%= link_to "Otroser", root_path %> </H1>
  <h1> <%= @serie.name.upcase %></h1>
  <h4> <%= @serie.description %> </h4>
</div>

<div class="images-container" data-controller="images-size">
    <% @serie.photos.each do |photo| %>
      <img src="<%= cl_image_path(photo.key) + ".jpg"  %>" class="big-images" data-images-size-target="image">
    <% end %>
</div>
