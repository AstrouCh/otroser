<div class="title-serie">
  <H1 id="otroser-title"> <%= link_to "Otroser", root_path %> </H1>
  <h2> <%= @serie.name.upcase %></h2>
  <h4> <%= @serie.description %> </h4>
</div>

<div class="images-container">
    <% @serie.photos.each do |photo| %>
      <img src="<%= cl_image_path(photo.key) + ".jpg"  %>" class="big-images" data-controller="images-size">
    <% end %>
</div>
