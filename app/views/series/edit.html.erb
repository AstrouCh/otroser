  <H1 id="otroser-title"> <%= link_to "Otroser", root_path %> </H1>

  <div class="forms-container">
    <h1>Actualizar la serie <%= @serie.name %></h1>
    <%= simple_form_for(@serie, url: update_series_path(@serie)) do |f| %>
      <%= f.input :name %>
      <%= f.input :description %>
      <%= f.input :photos, as: :file, input_html: { multiple: true } %>
      <%= f.submit %>
    <% end %>
  </div>
